  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@paljs/create": "^2.8.3",
    "@paljs/display": "^2.5.4",
    "@paljs/generator": "^2.8.4",
    "@paljs/schema": "^2.8.1",
    "@prisma/sdk": "2.12.0",
    "cross-spawn": "^7.0.3",
    "enquirer": "^2.3.6",
    "fs-extra": "^9.0.1",
    "pkg-dir": "^5.0.0",
    "prettier": "^2.1.2",
    "tslib": "^2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/fs-extra": "^9.0.4",
    "@types/mocha": "^8",
    "@types/node": "14.14.7",
    "chai": "^4",
    "eslint": "^7.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.2",
    "globby": "^11",
    "mocha": "^8",
    "nyc": "^15",
    "ts-node": "^9",
    "typescript": "4.0.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://paljs.com",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "pal",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "paljs/prisma-tools",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "a910803e8f7e689fea92ddbea48f63ae9290bab5"
